@model IEnumerable<dynamic>  

@{
    ViewData["Title"] = "All Events Created by Users";
}

<h2>All Events</h2>

<table class="table">
    <thead>
    <tr>
        <th>Event Name</th>
        <th>User Email</th>
            <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EventName</td>
            <td>@item.UserEmail</td>
                <td>
                    <!-- Add a button or link to navigate to the event details -->
                    <a href="@Url.Action("Details", "Admin", new { eventId = item.EventId })" class="btn btn-info">
                        Details
                    </a>
                </td>
        </tr>
    }
    </tbody>
</table>

